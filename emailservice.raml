                  "properties" : {
                    "key" : {
                      "description" : "The unique key of the template attribute.",
                      "type" : "string",
                      "pattern" : "^[a-zA-Z][a-zA-Z0-9_-]*$",
                      "maxLength" : 36,
                      "minLength" : 1
                    },
                    "value" : {
                      "description" : "The value for the template attribute.",
                      "oneOf" : [ {
                        "type" : "number"
                      }, {
                        "type" : "boolean"
                      }, {
                        "type" : "array"
                      }, {
                        "type" : "object",
                        "additionalProperties" : true
                      }, {
                        "type" : "string"
                      } ],
                      "maxLength" : 10000
                    }
                  },
                  "required" : [ "key", "value" ]
                }
    - 
        attributeDefinition: |
                {
                  "$schema" : "http://json-schema.org/draft-04/schema#",
                  "title" : "Template attribute definition",
                  "description" : "Used to mark a template attribute as mandatory or optional.",
                  "type" : "object",
                  "properties" : {
                    "key" : {
                      "description" : "The unique key of the template attribute.",
                      "type" : "string",
                      "pattern" : "^[a-zA-Z][a-zA-Z0-9_-]*$",
                      "maxLength" : 36,
                      "minLength" : 1
                    },
                    "mandatory" : {
                      "description" : "True if it is mandatory, false if it is optional.",
                      "type" : "boolean"
                    }
                  },
                  "required" : [ "key", "mandatory" ]
                }
    - 
