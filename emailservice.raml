/{tenant}/templates/{client}: 
    displayName: Manage email templates of a particular client
    description: Manages email templates for rendering of emails to be sent by the /{tenant}/send resource.
    uriParameters: 
        tenant: 
            description: The project that requests this resource. Must match the project that is associated with the access token in the Authorization header.
            type: string
            required: true
            repeat: false
            pattern: "[a-z][a-z0-9]+"
            minLength: 3
            maxLength: 16
        client: 
            description: The owner of the template(s).
            type: string
            required: true
            repeat: false
            pattern: "[a-z0-9-.]*"
            minLength: 6
            maxLength: 49
    type: collection
    is: [oauth2, restricted]
    get: 
        description: |
            Returns a list with all of the email templates and their metadata for a given project and client.
            The caller must have the `hybris.email_view` or `hybris.email_manage` scope assigned.
            This method uses a paging mechanism. If no paging parameters are provided, the default values are used.
        headers: 
            Authorization: 
                description: |
                    Used to send a valid OAuth2 access token.
                type: string
                required: true
                repeat: false
                example: Bearer access_token
        queryParameters: 
            pageNumber: 
                description: |
                    The page number to be retrieved where the size of the pages must be specified by the pageSize parameter.
                    The number of the first page is 1.
                type: integer
                required: false
                repeat: false
                minimum: 1
                default: "1"
                example: "1"
