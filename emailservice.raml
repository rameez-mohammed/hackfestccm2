        /{fileType}: 
            displayName: Manage a template file
            description: Manages Velocity template files that represent the subject or body of an email template. These may be available in several localized variants.
            uriParameters: 
                fileType: 
                    description: The type of the template file. This can be either "subject" or "body", optionally followed by an underscore and a locale code.
                    type: string
                    required: true
                    repeat: false
                    pattern: "(body|subject)(_[a-zA-Z]{2}){0,2}"
                    example: body_en_us
            is: [oauth2, restricted]
            put: 
                description: Creates or updates the body/subject file of this email template for a specific locale. The caller must have the `hybris.email_manage` scope assigned. Files over 50MB cannot be uploaded.
